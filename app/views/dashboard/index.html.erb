
<title>NASA Challenge</title>

<h3>Dashboard</h3>


<% if @projects.empty? %>
  <p><strong>There are not Projects :( </strong></p>
<% end %>




<% @projects.each do |project| %>
<div class="well">
<div class="row">
	<% from_time = Time.now %>


<div class="row-title"><h3><%= project.name %></h3></div>
<div class="row-description"><%= project.description %></div>
<div class="row-url"><%= project.project_url %></div>
<div class="row-url"><%= project.source_code_url %></div>
<div class="row-url">Project Type: <%= project.vcs %></div>
<br>
<div class="row-ending">Added <%= distance_of_time_in_words(project.created_at, from_time) %> Ago</div>


</div>

</div>
<% end %>
